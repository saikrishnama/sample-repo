CREATE TABLE IF NOT EXISTS my_catalog.audit_schema.grant_audit_log (
  timestamp TIMESTAMP,
  schema STRING,
  object_name STRING,
  object_type STRING,       -- SCHEMA, TABLE, FUNCTION
  privilege STRING,
  grantee STRING,           -- user, group, or service principal name
  grantee_type STRING       -- "user", "group", or "service_principal"
)
